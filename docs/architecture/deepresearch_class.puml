@startuml
title DeepResearch Agent - Class Diagram

class DeepResearchCrew {
  + __init__(research_topic: str)
  + crew(): Crew
  + generate_report(prompt: str): str
}

class Agent {
  + role
  + goal
  + backstory
  + tools
}

class Task {
  + description
  + expected_output
  + agent
}

class Tools {
  + SerperDevTool
  + GithubSearchTool
}

DeepResearchCrew o-- Agent
DeepResearchCrew o-- Task
Agent o-- Tools

@enduml
